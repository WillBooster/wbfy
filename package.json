{
  "name": "willboosterify",
  "version": "0.0.0-development",
  "private": false,
  "description": "A tool for applying WillBooster's conventional configures to npm packages",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": {
    "willboosterify": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettier",
    "lint": "eslint \"./{packages/*/,}{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "rimraf dist && yarn build",
    "prettier": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "./bin/run",
    "test-release": "dotenv yarn semantic-release -d",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-help": "2.2.3",
    "deepmerge": "4.2.2",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "js-yaml": "3.13.1",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@types/fs-extra": "8.0.1",
    "@types/glob": "7.1.1",
    "@types/js-yaml": "3.12.2",
    "@types/node-fetch": "2.5.4",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "@willbooster/eslint-config-ts": "2.3.2",
    "@willbooster/prettier-config": "2.3.2",
    "@willbooster/renovate-config": "2.3.2",
    "dotenv-cli": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.1",
    "lint-staged": "10.0.4",
    "prettier": "1.19.1",
    "rimraf": "3.0.1",
    "semantic-release": "17.0.1",
    "sort-package-json": "1.39.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "bin": "willboosterify"
  }
}
