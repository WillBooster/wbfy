{
  "name": "wbfy",
  "version": "0.0.0-semantically-released",
  "description": "A tool for applying WillBooster's conventional configures to npm packages",
  "repository": "github:WillBooster/wbfy",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.cjs",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "node esbuild.js",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{__tests__,scripts,src}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "release": "yarn build && semantic-release",
    "start": "dotenv -- node -r esbuild-register src",
    "start-prod": "yarn build && yarn wbfy",
    "test": "yarn jest --color __tests__",
    "test/ci-setup": "cd .. && pwd && rm -Rf test-fixtures-for-wbfy && git clone https://github.com/WillBooster/test-fixtures-for-wbfy.git",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "dependencies": {
    "@octokit/core": "4.1.0",
    "deepmerge": "4.2.2",
    "dotenv": "16.0.3",
    "dotenv-cli": "6.0.0",
    "glob": "8.0.3",
    "js-yaml": "4.1.0",
    "libsodium-wrappers": "0.7.10",
    "lodash.clonedeep": "4.5.0",
    "minimal-promise-pool": "1.2.0",
    "node-fetch": "3.3.0",
    "simple-git": "3.15.1",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.3",
    "@types/js-yaml": "4.0.5",
    "@types/libsodium-wrappers": "0.7.10",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.10",
    "@types/node-fetch": "3.0.3",
    "@types/yargs": "17.0.15",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@willbooster/eslint-config-ts": "10.0.8",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.4.2",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "esbuild": "0.15.16",
    "esbuild-register": "3.4.1",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-class-members": "1.16.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "45.0.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.0.4",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.8.0",
    "semantic-release": "19.0.5",
    "sort-package-json": "2.1.0",
    "typescript": "4.9.3"
  },
  "packageManager": "yarn@4.0.0-rc.32",
  "engines": {
    "node": ">=16.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
