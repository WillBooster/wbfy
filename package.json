{
  "name": "wbfy",
  "version": "0.0.0-semantically-released",
  "description": "A tool for applying WillBooster's conventional configures to npm packages",
  "repository": "github:WillBooster/wbfy",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "bin": "./bin/wbfy.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts app",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "release": "yarn build && semantic-release",
    "start": "dotenv -- build-ts run src/index.ts --",
    "start-prod": "yarn build && yarn wbfy",
    "test": "vitest tests",
    "test/ci-setup": "cd .. && pwd && rm -Rf test-fixtures-for-wbfy && git clone https://github.com/WillBooster/test-fixtures-for-wbfy.git",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@octokit/core": "4.2.0",
    "@willbooster/shared-lib": "3.0.1",
    "deepmerge": "4.3.1",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.2.1",
    "globby": "13.1.4",
    "js-yaml": "4.1.0",
    "libsodium-wrappers": "0.7.11",
    "lodash.clonedeep": "4.5.0",
    "minimal-promise-pool": "2.1.8",
    "simple-git": "3.18.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/eslint": "8.37.0",
    "@types/jest": "29.5.1",
    "@types/js-yaml": "4.0.5",
    "@types/libsodium-wrappers": "0.7.10",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.16.5",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@willbooster/eslint-config-ts": "10.1.0",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.5.0",
    "@yarnpkg/core": "4.0.0-rc.43",
    "build-ts": "5.7.13",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-class-members": "1.17.1",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "46.0.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.8.8",
    "semantic-release": "21.0.2",
    "sort-package-json": "2.4.1",
    "ts-node": "10.9.1",
    "type-fest": "3.10.0",
    "typescript": "5.0.4",
    "vitest": "0.31.0"
  },
  "packageManager": "yarn@4.0.0-rc.43",
  "engines": {
    "node": ">=16.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
