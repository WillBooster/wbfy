{
  "name": "willboosterify",
  "version": "0.0.0-development",
  "description": "A tool for applying WillBooster's conventional configures to npm packages",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "scripts": {
    "build": "tsc",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "rimraf dist && yarn build",
    "start": "babel-node -x .ts -i /node_modules/ src",
    "test-release": "dotenv yarn semantic-release -d",
    "typecheck": "tsc --noEmit --Pretty",
    "prettify": "prettier --color --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "core-js": "3.15.2",
    "deepmerge": "4.2.2",
    "glob": "7.1.7",
    "js-yaml": "4.1.0",
    "lodash.clonedeep": "4.5.0",
    "node-fetch": "2.6.1",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/node": "7.14.7",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-numeric-separator": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@types/eslint": "7.28.0",
    "@types/glob": "7.1.4",
    "@types/js-yaml": "4.0.2",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/micromatch": "4.0.2",
    "@types/node-fetch": "2.5.12",
    "@types/yargs": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "@willbooster/eslint-config-ts": "6.3.0",
    "@willbooster/prettier-config": "6.2.1",
    "@willbooster/renovate-config": "6.3.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "dotenv-cli": "4.0.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.23.4",
    "husky": "6.0.0",
    "lint-staged": "11.1.0",
    "micromatch": "4.0.4",
    "pinst": "2.1.6",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.4",
    "sort-package-json": "1.50.0",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
