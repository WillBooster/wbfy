{
  "name": "wbfy",
  "version": "0.0.0-semantically-released",
  "description": "A tool for applying WillBooster's conventional configures to npm packages",
  "repository": "github:WillBooster/wbfy",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "bin": "./bin/wbfy.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts app",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "release": "yarn build && semantic-release",
    "start": "build-ts run src/index.ts --env .env --",
    "start-prod": "yarn build && yarn wbfy",
    "test": "vitest tests",
    "test/ci-setup": "cd .. && pwd && rm -Rf test-fixtures-for-wbfy && git clone https://github.com/WillBooster/test-fixtures-for-wbfy.git",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@octokit/core": "5.0.0",
    "@willbooster/shared-lib": "3.4.2",
    "deepmerge": "4.3.1",
    "dotenv": "16.3.1",
    "globby": "13.2.2",
    "js-yaml": "4.1.0",
    "libsodium-wrappers": "0.7.11",
    "lodash.clonedeep": "4.5.0",
    "minimal-promise-pool": "3.0.1",
    "simple-git": "3.19.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/eslint": "8.44.2",
    "@types/jest": "29.5.3",
    "@types/js-yaml": "4.0.5",
    "@types/libsodium-wrappers": "0.7.10",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.17.4",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@willbooster/eslint-config-ts": "10.4.0",
    "@willbooster/prettier-config": "9.1.1",
    "@yarnpkg/core": "4.0.0-rc.48",
    "build-ts": "8.2.0",
    "conventional-changelog-conventionalcommits": "6.1.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-sort-class-members": "1.18.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "lint-staged": "13.2.3",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "3.0.1",
    "semantic-release": "21.0.7",
    "sort-package-json": "2.5.1",
    "ts-node": "10.9.1",
    "type-fest": "4.2.0",
    "typescript": "5.1.6",
    "vitest": "0.34.1"
  },
  "packageManager": "yarn@4.0.0-rc.48",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
