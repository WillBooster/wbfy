{
  "name": "willboosterify",
  "version": "0.0.0-development",
  "private": false,
  "description": "A tool for applying WillBooster's conventional configures to npm packages",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "scripts": {
    "build": "tsc",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettier",
    "lint": "eslint \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\" --color",
    "lint-fix": "yarn lint --fix",
    "prepack": "rimraf dist && yarn build",
    "prettier": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\" --color",
    "start": "yarn ts-node src",
    "test-release": "dotenv yarn semantic-release -d",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "deepmerge": "4.2.2",
    "fs-extra": "9.0.1",
    "glob": "7.1.6",
    "js-yaml": "3.14.0",
    "node-fetch": "2.6.1",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.2",
    "@types/glob": "7.1.3",
    "@types/js-yaml": "3.12.5",
    "@types/node-fetch": "2.5.7",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "@willbooster/eslint-config-ts": "5.0.0",
    "@willbooster/prettier-config": "5.0.0",
    "@willbooster/renovate-config": "5.0.0",
    "dotenv-cli": "4.0.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.2.3",
    "sort-package-json": "1.46.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
