{
  "name": "wbfy",
  "version": "0.0.0-semantically-released",
  "description": "A tool for applying WillBooster's conventional configures to npm packages",
  "repository": "github:WillBooster/wbfy",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.cjs",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "node esbuild.js",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "release": "yarn build && semantic-release",
    "setup-ci": "cd .. && pwd && rm -Rf test-fixtures-for-wbfy && git clone https://github.com/WillBooster/test-fixtures-for-wbfy.git",
    "start": "node -r esbuild-register src",
    "test": "yarn jest --color __tests__",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "dependencies": {
    "deepmerge": "4.2.2",
    "glob": "7.2.0",
    "js-yaml": "4.1.0",
    "lodash.clonedeep": "4.5.0",
    "minimal-promise-pool": "1.1.4",
    "node-fetch": "3.2.3",
    "simple-git": "3.3.0",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/glob": "7.2.0",
    "@types/jest": "27.4.1",
    "@types/js-yaml": "4.0.5",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/micromatch": "4.0.2",
    "@types/node": "16.11.26",
    "@types/node-fetch": "3.0.3",
    "@types/yargs": "17.0.9",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "@willbooster/eslint-config-ts": "9.0.0",
    "@willbooster/prettier-config": "9.0.0",
    "@willbooster/renovate-config": "9.2.0",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "esbuild": "0.14.25",
    "esbuild-register": "3.3.2",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-sort-class-members": "1.14.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "micromatch": "4.0.4",
    "pinst": "3.0.0",
    "prettier": "2.5.1",
    "semantic-release": "19.0.2",
    "sort-package-json": "1.54.0",
    "typescript": "4.6.2"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "packageManager": "yarn@3.2.0"
}
